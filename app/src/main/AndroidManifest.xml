<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adminpanelapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcherm"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AdminPanelApplication">
        <activity android:name=".UpdateLecturerActivity"></activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".Splash"
            android:noHistory="true"
            android:parentActivityName=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddLecturer" />
        <activity
            android:name=".UpdateFaculty"
            android:label="@string/change_faculty"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DeleteNotice"
            android:label="@string/remove_notice"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".UploadImages"
            android:label="@string/upload_image"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".UploadPdf"
            android:label="@string/upload_pdf"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".UploadNotice"
            android:label="@string/upload_notice"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>